;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ns.bistorm.local. root.ns.bistorm.local. (
			      5		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
; 

@               IN      NS      ns.
%%LAST_D%%      IN      PTR     ns.bistorm.local.
%%LAST_D%%      IN      PTR     sandy.bistorm.local.

